= form_for(@musician) do |f|
  - if @musician.errors.any?
    #error_explanation
      = render partial: 'partials/error_wrapper', locals: { resource: @musician }

  = f.label :name
  = f.text_field :name
  = f.label :last_name
  = f.text_field :last_name
  = f.label :identification
  = f.text_field :identification
  = f.label :phone
  = f.text_field :phone
  = f.label :image
  = f.file_field :image
  = f.label :instruments
  - for instrument in Instrument.all
    %div
      = check_box_tag "musician[instrument_ids][]", instrument.id, @musician.instruments.include?(instrument)
  - for genre in Genre.all
    %div
      = check_box_tag "musician[genre_ids][]", genre.id, @musician.genres.include?(genre)
      = genre.name
  = f.label :event_types
  - for type in EventType.all
    %div
      = check_box_tag "musician[event_type_ids][]", type.id, @musician.event_types.include?(type)
      = type.name

  .actions
    = f.submit
